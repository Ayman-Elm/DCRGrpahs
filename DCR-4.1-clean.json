{
  "DCRModel": [
    {
      "title": "Jobcenter Offer (AB 101) Process",
      "description": "Workflow for preparing, confirming, executing, and handling complaints about a jobcenter offer (AB 101) to a citizen.",
      "type": "DCRModel",
      "roles": [],
      "events": [
        {
          "id": "JOBCENTER_CREATE_OFFER",
          "label": "Jobcenter creates offer"
        },
        {
          "id": "JOBCENTER_SEND_OFFER_TO_CITIZEN",
          "label": "Jobcenter sends offer to citizen"
        },
        {
          "id": "CITIZEN_RECEIVE_OFFER",
          "label": "Citizen receives offer"
        },
        {
          "id": "CITIZEN_SIGN_CONFIRMATION",
          "label": "Citizen signs confirmation"
        },
        {
          "id": "CITIZEN_RETURN_CONFIRMATION",
          "label": "Citizen returns signed confirmation"
        },
        {
          "id": "OPTIONAL_CREATE_INDIVIDUAL_AGREEMENT",
          "label": "Create individual agreement (optional)"
        },
        {
          "id": "CITIZEN_SIGN_INDIVIDUAL_AGREEMENT",
          "label": "Citizen signs individual agreement"
        },
        {
          "id": "CITIZEN_START_OFFER",
          "label": "Citizen starts in offer"
        },
        {
          "id": "CITIZEN_REPORT_SICKNESS",
          "label": "Citizen reports sickness"
        },
        {
          "id": "CITIZEN_REQUEST_HOLIDAY",
          "label": "Citizen requests holiday"
        },
        {
          "id": "OFFERPLACE_REPORT_ABSENCE_OR_END",
          "label": "Offer place reports absence or ending"
        },
        {
          "id": "CITIZEN_FILE_COMPLAINT",
          "label": "Citizen files complaint"
        },
        {
          "id": "JOBCENTER_FORWARD_COMPLAINT",
          "label": "Jobcenter forwards complaint"
        },
        {
          "id": "OFFER_ENDS_NORMALLY",
          "label": "Offer ends normally"
        }
      ],
      "rules": [
        {
          "type": "condition",
          "source": "JOBCENTER_CREATE_OFFER",
          "target": "JOBCENTER_SEND_OFFER_TO_CITIZEN",
          "description": "Offer must be created before sending to citizen"
        },
        {
          "type": "condition",
          "source": "JOBCENTER_SEND_OFFER_TO_CITIZEN",
          "target": "CITIZEN_RECEIVE_OFFER",
          "description": "Offer must be sent before citizen can receive it"
        },
        {
          "type": "condition",
          "source": "CITIZEN_RECEIVE_OFFER",
          "target": "CITIZEN_START_OFFER",
          "description": "Citizen must have received offer before starting"
        },
        {
          "type": "condition",
          "source": "CITIZEN_RECEIVE_OFFER",
          "target": "CITIZEN_SIGN_CONFIRMATION",
          "description": "Citizen must receive offer before signing confirmation"
        },
        {
          "type": "condition",
          "source": "CITIZEN_RECEIVE_OFFER",
          "target": "CITIZEN_RETURN_CONFIRMATION",
          "description": "Citizen must receive offer before returning confirmation"
        },
        {
          "type": "condition",
          "source": "CITIZEN_SIGN_CONFIRMATION",
          "target": "CITIZEN_RETURN_CONFIRMATION",
          "description": "Confirmation must be signed before it can be returned"
        },
        {
          "type": "milestone",
          "source": "CITIZEN_RETURN_CONFIRMATION",
          "target": "CITIZEN_START_OFFER",
          "description": "Returning the signed confirmation formalises the start of the offer"
        },
        {
          "type": "condition",
          "source": "JOBCENTER_CREATE_OFFER",
          "target": "OPTIONAL_CREATE_INDIVIDUAL_AGREEMENT",
          "description": "Individual agreement can only be created after offer created"
        },
        {
          "type": "condition",
          "source": "OPTIONAL_CREATE_INDIVIDUAL_AGREEMENT",
          "target": "CITIZEN_SIGN_INDIVIDUAL_AGREEMENT",
          "description": "Citizen can only sign if agreement is created"
        },
        {
          "type": "milestone",
          "source": "CITIZEN_START_OFFER",
          "target": "OPTIONAL_CREATE_INDIVIDUAL_AGREEMENT",
          "description": "Individual agreement should be created before or soon after starting offer"
        },
        {
          "type": "condition",
          "source": "CITIZEN_START_OFFER",
          "target": "CITIZEN_REPORT_SICKNESS",
          "description": "Citizen can only report sickness after starting offer"
        },
        {
          "type": "condition",
          "source": "CITIZEN_RECEIVE_OFFER",
          "target": "CITIZEN_REQUEST_HOLIDAY",
          "description": "Holiday request only possible after receiving offer"
        },
        {
          "type": "condition",
          "source": "CITIZEN_START_OFFER",
          "target": "OFFERPLACE_REPORT_ABSENCE_OR_END",
          "description": "Offer place can only report absence or ending after citizen has started"
        },
        {
          "type": "condition",
          "source": "CITIZEN_RECEIVE_OFFER",
          "target": "CITIZEN_FILE_COMPLAINT",
          "description": "Citizen can only file complaint after receiving offer"
        },
        {
          "type": "response",
          "source": "CITIZEN_RECEIVE_OFFER",
          "target": "CITIZEN_FILE_COMPLAINT",
          "description": "Complaint must be filed within 4 weeks of receiving offer",
          "duration": "P4W"
        },
        {
          "type": "condition",
          "source": "CITIZEN_FILE_COMPLAINT",
          "target": "JOBCENTER_FORWARD_COMPLAINT",
          "description": "Forwarding to Ankestyrelsen only possible after complaint filed"
        },
        {
          "type": "condition",
          "source": "CITIZEN_START_OFFER",
          "target": "OFFER_ENDS_NORMALLY",
          "description": "Offer can only end after citizen started"
        }
      ],
      "marking": {
        "executed": [],
        "included": [
          "JOBCENTER_CREATE_OFFER",
          "JOBCENTER_SEND_OFFER_TO_CITIZEN",
          "CITIZEN_RECEIVE_OFFER",
          "CITIZEN_SIGN_CONFIRMATION",
          "CITIZEN_RETURN_CONFIRMATION",
          "OPTIONAL_CREATE_INDIVIDUAL_AGREEMENT",
          "CITIZEN_SIGN_INDIVIDUAL_AGREEMENT",
          "CITIZEN_START_OFFER",
          "CITIZEN_REPORT_SICKNESS",
          "CITIZEN_REQUEST_HOLIDAY",
          "OFFERPLACE_REPORT_ABSENCE_OR_END",
          "CITIZEN_FILE_COMPLAINT",
          "JOBCENTER_FORWARD_COMPLAINT",
          "OFFER_ENDS_NORMALLY"
        ],
        "pending": []
      }
    }
  ]
}