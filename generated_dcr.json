{
  "DCRModel": [
    {
      "title": "Jobcenter Offer (AB 101) Process",
      "description": "Process for creating, communicating, accepting, participating in, and potentially complaining about a Jobcenter offer (AB 101) under 'Lov om en aktiv besk√¶ftigelsesindsats'.",
      "type": "DCRModel",
      "roles": [],
      "events": [
        {
          "id": "JOBCENTER_CREATE_OFFER",
          "label": "Jobcenter creates offer (AB 101)"
        },
        {
          "id": "JOBCENTER_SEND_OFFER_TO_CITIZEN",
          "label": "Jobcenter sends offer to citizen"
        },
        {
          "id": "CITIZEN_RECEIVE_OFFER",
          "label": "Citizen receives offer"
        },
        {
          "id": "CITIZEN_SIGN_CONFIRMATION",
          "label": "Citizen signs confirmation (blad 2)"
        },
        {
          "id": "CITIZEN_RETURN_CONFIRMATION",
          "label": "Citizen returns signed confirmation (blad 2)"
        },
        {
          "id": "OPTIONAL_CREATE_INDIVIDUAL_AGREEMENT",
          "label": "Create individual agreement (blad 3, optional)"
        },
        {
          "id": "CITIZEN_SIGN_INDIVIDUAL_AGREEMENT",
          "label": "Citizen signs individual agreement (blad 3)"
        },
        {
          "id": "CITIZEN_START_OFFER",
          "label": "Citizen starts in offer"
        },
        {
          "id": "CITIZEN_REPORT_SICKNESS",
          "label": "Citizen reports sickness during offer"
        },
        {
          "id": "CITIZEN_REQUEST_HOLIDAY",
          "label": "Citizen requests holiday during offer"
        },
        {
          "id": "OFFERPLACE_REPORT_ABSENCE_OR_END",
          "label": "Offer place reports absence or early end"
        },
        {
          "id": "OFFER_ENDS_NORMALLY",
          "label": "Offer ends normally or is formally closed"
        },
        {
          "id": "CITIZEN_FILE_COMPLAINT",
          "label": "Citizen files complaint about offer"
        },
        {
          "id": "JOBCENTER_FORWARD_COMPLAINT",
          "label": "Jobcenter forwards complaint to Ankestyrelsen"
        },
        {
          "id": "LATE_COMPLAINT_CUTOFF",
          "label": "Complaint deadline (4 weeks after receipt) passes"
        }
      ],
      "rules": [
        {
          "type": "condition",
          "source": "JOBCENTER_CREATE_OFFER",
          "target": "JOBCENTER_SEND_OFFER_TO_CITIZEN",
          "description": "Offer must be created before it can be sent to the citizen"
        },
        {
          "type": "condition",
          "source": "JOBCENTER_SEND_OFFER_TO_CITIZEN",
          "target": "CITIZEN_RECEIVE_OFFER",
          "description": "Offer must be sent before the citizen can receive it"
        },
        {
          "type": "condition",
          "source": "CITIZEN_RECEIVE_OFFER",
          "target": "CITIZEN_SIGN_CONFIRMATION",
          "description": "Citizen must receive the offer before signing confirmation"
        },
        {
          "type": "condition",
          "source": "CITIZEN_SIGN_CONFIRMATION",
          "target": "CITIZEN_RETURN_CONFIRMATION",
          "description": "Citizen must sign confirmation before returning it"
        },
        {
          "type": "condition",
          "source": "CITIZEN_RECEIVE_OFFER",
          "target": "CITIZEN_START_OFFER",
          "description": "Citizen must receive the offer before starting in the offer"
        },
        {
          "type": "condition",
          "source": "CITIZEN_RECEIVE_OFFER",
          "target": "CITIZEN_FILE_COMPLAINT",
          "description": "Citizen can only complain after having received the offer"
        },
        {
          "type": "response",
          "source": "CITIZEN_SIGN_CONFIRMATION",
          "target": "CITIZEN_RETURN_CONFIRMATION",
          "description": "After signing, the citizen is expected to return the confirmation shortly"
        },
        {
          "type": "condition",
          "source": "JOBCENTER_CREATE_OFFER",
          "target": "OPTIONAL_CREATE_INDIVIDUAL_AGREEMENT",
          "description": "Individual agreement can only be created after the offer is created"
        },
        {
          "type": "condition",
          "source": "OPTIONAL_CREATE_INDIVIDUAL_AGREEMENT",
          "target": "CITIZEN_SIGN_INDIVIDUAL_AGREEMENT",
          "description": "Citizen can only sign the individual agreement after it is created"
        },
        {
          "type": "milestone",
          "source": "CITIZEN_START_OFFER",
          "target": "OPTIONAL_CREATE_INDIVIDUAL_AGREEMENT",
          "description": "Individual agreement should be created before or shortly after the citizen starts in the offer"
        },
        {
          "type": "condition",
          "source": "CITIZEN_SIGN_CONFIRMATION",
          "target": "CITIZEN_START_OFFER",
          "description": "Citizen is expected to have signed confirmation before or when starting the offer"
        },
        {
          "type": "condition",
          "source": "CITIZEN_START_OFFER",
          "target": "CITIZEN_REPORT_SICKNESS",
          "description": "Sickness can only be reported in relation to an active offer"
        },
        {
          "type": "condition",
          "source": "CITIZEN_START_OFFER",
          "target": "CITIZEN_REQUEST_HOLIDAY",
          "description": "Holiday can only be requested while the citizen is in the offer"
        },
        {
          "type": "condition",
          "source": "CITIZEN_START_OFFER",
          "target": "OFFERPLACE_REPORT_ABSENCE_OR_END",
          "description": "Offer place can only report absence or early end after the citizen has started"
        },
        {
          "type": "condition",
          "source": "CITIZEN_START_OFFER",
          "target": "OFFER_ENDS_NORMALLY",
          "description": "The offer can only end after it has been started"
        },
        {
          "type": "exclude",
          "source": "OFFER_ENDS_NORMALLY",
          "target": "CITIZEN_REPORT_SICKNESS",
          "description": "After the offer ends normally, sickness during the offer can no longer be reported"
        },
        {
          "type": "exclude",
          "source": "OFFER_ENDS_NORMALLY",
          "target": "CITIZEN_REQUEST_HOLIDAY",
          "description": "After the offer ends normally, holiday during the offer can no longer be requested"
        },
        {
          "type": "exclude",
          "source": "OFFER_ENDS_NORMALLY",
          "target": "OFFERPLACE_REPORT_ABSENCE_OR_END",
          "description": "After the offer has ended, the offer place cannot report further absence or early end"
        },
        {
          "type": "exclude",
          "source": "OFFERPLACE_REPORT_ABSENCE_OR_END",
          "target": "CITIZEN_REPORT_SICKNESS",
          "description": "If the offer place reports early end, later sickness during the offer is no longer relevant"
        },
        {
          "type": "exclude",
          "source": "OFFERPLACE_REPORT_ABSENCE_OR_END",
          "target": "CITIZEN_REQUEST_HOLIDAY",
          "description": "If the offer place reports early end, holiday during the offer cannot be requested afterwards"
        },
        {
          "type": "exclude",
          "source": "OFFERPLACE_REPORT_ABSENCE_OR_END",
          "target": "OFFER_ENDS_NORMALLY",
          "description": "If the offer ends prematurely, it does not also end normally"
        },
        {
          "type": "response",
          "source": "CITIZEN_RECEIVE_OFFER",
          "target": "LATE_COMPLAINT_CUTOFF",
          "description": "Receiving the offer starts the 4-week complaint deadline",
          "duration": "P4W"
        },
        {
          "type": "condition",
          "source": "LATE_COMPLAINT_CUTOFF",
          "target": "CITIZEN_FILE_COMPLAINT",
          "description": "Complaint must be filed before the 4-week deadline passes"
        },
        {
          "type": "exclude",
          "source": "LATE_COMPLAINT_CUTOFF",
          "target": "CITIZEN_FILE_COMPLAINT",
          "description": "After the 4-week deadline passes, complaints are normally rejected as late"
        },
        {
          "type": "condition",
          "source": "CITIZEN_FILE_COMPLAINT",
          "target": "JOBCENTER_FORWARD_COMPLAINT",
          "description": "Jobcenter can only forward a complaint that has been filed by the citizen"
        }
      ],
      "marking": {
        "executed": [],
        "included": [
          "JOBCENTER_CREATE_OFFER",
          "JOBCENTER_SEND_OFFER_TO_CITIZEN",
          "CITIZEN_RECEIVE_OFFER",
          "CITIZEN_SIGN_CONFIRMATION",
          "CITIZEN_RETURN_CONFIRMATION",
          "OPTIONAL_CREATE_INDIVIDUAL_AGREEMENT",
          "CITIZEN_SIGN_INDIVIDUAL_AGREEMENT",
          "CITIZEN_START_OFFER",
          "CITIZEN_REPORT_SICKNESS",
          "CITIZEN_REQUEST_HOLIDAY",
          "OFFERPLACE_REPORT_ABSENCE_OR_END",
          "OFFER_ENDS_NORMALLY",
          "CITIZEN_FILE_COMPLAINT",
          "JOBCENTER_FORWARD_COMPLAINT",
          "LATE_COMPLAINT_CUTOFF"
        ],
        "pending": []
      }
    }
  ]
}